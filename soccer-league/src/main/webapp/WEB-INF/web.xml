<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ListCustomer</servlet-name>
		<display-name>ListCustomer</display-name>
		<description></description>
		<servlet-class>comm.example.view.ListCustomer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddCustomer</servlet-name>
		<display-name>AddCustomer</display-name>
		<description></description>
		<servlet-class>comm.example.controller.AddCustomer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ErrorServlet</servlet-name>
		<display-name>ErrorServlet</display-name>
		<description></description>
		<servlet-class>comm.example.view.ErrorServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SuccessServlet</servlet-name>
		<display-name>SuccessServlet</display-name>
		<description></description>
		<servlet-class>comm.example.view.SuccessServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<display-name>RegisterServlet</display-name>
		<description></description>
		<servlet-class>comm.example.view.RegisterServlet</servlet-class>
		<init-param>
		<param-name>type</param-name>
		<param-value>Unknown,New,Returning</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>DeleteServlet</servlet-name>
		<display-name>DeleteServlet</display-name>
		<description></description>
		<servlet-class>comm.example.view.DeleteServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Delete</servlet-name>
		<display-name>Delete</display-name>
		<description></description>
		<servlet-class>comm.example.controller.Delete</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListCustomer</servlet-name>
		<url-pattern>/list_customer.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddCustomer</servlet-name>
		<url-pattern>/add_customer.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ErrorServlet</servlet-name>
		<url-pattern>/error.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SuccessServlet</servlet-name>
		<url-pattern>/success.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/register.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteServlet</servlet-name>
		<url-pattern>/delete.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	<welcome-file-list>

		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
